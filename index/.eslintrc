{
  "extends":[
    "airbnb",
    "plugin:import/errors"
  ],
  "plugins": ["import"],
  "env": {
    "node":true,
    "es6": true
  },
  "rules": {
    "no-console":["off"],
    "linebreak-style":["off"],
    "consistent-return":["off"]
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  }
}